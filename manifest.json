{
    "manifest_version": 3,
    "name": "Lum",
    "version": "0.0.1",
    "icons": {
        "128": "lum.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["content.js"]
        }
    ]

}